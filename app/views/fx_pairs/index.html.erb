<p id="notice"><%= notice %></p>

<h1>Fx Pairs</h1>

<table>
  <thead>
    <tr>
      <th>Base</th>
      <th>Target</th>
      <th>Rate</th>
      <th>Last updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fx_pairs.each do |fx_pair| %>
      <tr>
        <td><%= fx_pair.base %></td>
        <td><%= fx_pair.target %></td>
        <td><%= fx_pair.rate %></td>
        <td><%= fx_pair.last_updated_at %></td>
        <td><%= link_to 'Show', fx_pair %></td>
        <td><%= link_to 'Edit', edit_fx_pair_path(fx_pair) %></td>
        <td><%= link_to 'Destroy', fx_pair, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fx Pair', new_fx_pair_path %>
