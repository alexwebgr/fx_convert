var fx = $('#<%= "#{@fx_pair.base}_#{@fx_pair.target}" %>');
if(fx.length) {
  fx.replaceWith('<%= j render partial: 'dashboard/fx_pair', locals: {fx_pair: @fx_pair} %>');
}
else {
  $('#fx_pairs').append('<%= j render partial: 'dashboard/fx_pair', locals: {fx_pair: @fx_pair} %>')
    var empty = $('#fx_empty');
    if (empty.length) {
        empty.remove();
    }
}
